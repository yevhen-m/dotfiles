{
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "Monaco",
  "editor.highlightActiveIndentGuide": false,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {"comments": true, "other": true, "strings": true},
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "all",
  "editor.rulers": [79],
  "editor.scrollBeyondLastLine": true,
  "editor.smoothScrolling": true,
  "editor.wordBasedSuggestions": true,
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "gitlens.advanced.messages": {"suppressShowKeyBindingsNotice": true},
  "python.formatting.blackArgs": [
    "--quiet",
    "--line-length=79",
    "--skip-string-normalization",
    "--skip-numeric-underscore-normalization"
  ],
  "python.formatting.provider": "black",
  "python.linting.pylintEnabled": false,
  "python.venvPath": "~/.virtualenvs",
  "search.location": "panel",
  "search.smartCase": true,
  "search.usePCRE2": false,
  "telemetry.enableTelemetry": false,
  "terminal.explorerKind": "external",
  "terminal.external.osxExec": "iTerm.app",
  "window.clickThroughInactive": true,
  "window.nativeTabs": true,
  "window.restoreFullscreen": false,
  "window.restoreWindows": "all",
  "workbench.editor.swipeToNavigate": true,
  "workbench.editor.tabCloseButton": "left",
  "workbench.fontAliasing": "antialiased",
  "workbench.startupEditor": "none",
  "workbench.statusBar.feedback.visible": false,
  "workbench.colorCustomizations": {
    "statusBar.background": "#383838",
    "statusBar.noFolderBackground" : "#383838",
    "statusBar.debuggingBackground": "#383838"
  },
  "workbench.tree.horizontalScrolling": true,
  "window.zoomLevel": 0,
  "editor.wordWrap": "on",
  "diffEditor.renderSideBySide": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.tags": true,
    "**/tags": true,
    "**/__pycache__": true,
    "**/static/dist": true,
    "**/coverage*": true,
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/__pycache__": true,
    "**/static/dist": true,
  },
  "editor.scrollBeyondLastColumn": 0,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.settings.mode": "advanced",
  "gitlens.menus": {
    "editor": {
      "blame": true,
      "clipboard": true,
      "compare": true,
      "details": true,
      "history": true,
      "remote": true
    },
    "editorGroup": {
      "blame": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "editorTab": {
      "compare": true,
      "history": true,
      "remote": true
    },
    "explorer": {
      "compare": true,
      "history": true,
      "remote": true
    }
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.list.multiSelectModifier": "alt",
  "workbench.editor.openSideBySideDirection": "down",
  "search.runInExtensionHost": false,
  "editor.overviewRulerBorder": false,
  "clock.dateFormat": "HH:MM",
  "explorer.confirmDelete": false,
  "workbench.iconTheme": "vscode-great-icons",
  "extensions.showRecommendationsOnlyOnDemand": true,
  "search.useIgnoreFiles": true,
  "breadcrumbs.enabled": true,
  "editor.acceptSuggestionOnEnter": "off",
  "workbench.editor.revealIfOpen": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.blame.avatars": false,
  "gitlens.blame.highlight.locations": [
    "gutter",
    "line",
    "overview"
  ],
  "gitlens.hovers.avatars": false,
  "gitlens.hovers.annotations.over": "annotation",
  "gitlens.statusBar.reduceFlicker": true,
  "search.useGlobalIgnoreFiles": true,
  "search.showLineNumbers": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.panel.defaultLocation": "right",
  "gitlens.views.fileHistory.avatars": false,
  "gitlens.views.fileHistory.location": "scm",
  "gitlens.views.lineHistory.avatars": false,
  "gitlens.views.lineHistory.location": "scm",
  "gitlens.views.compare.files.layout": "tree",
  "gitlens.views.search.files.layout": "tree",
  "git.openDiffOnClick": false,
  "gitlens.views.compare.avatars": false,
  "gitlens.views.repositories.avatars": false,
  "gitlens.views.search.avatars": false,
  "vsnotes.defaultNotePath": "/Users/yevhen/.cache/vsnotes",
  "python.testing.autoTestDiscoverOnSaveEnabled": false,
  "python.testing.promptToConfigure": false,
  "search.useReplacePreview": false,
  "python.linting.flake8Enabled": true,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[csharp]": {
    "editor.defaultFormatter": "ms-vscode.csharp"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
