{
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "block",
  "editor.fontFamily": "Monaco",
  "editor.fontSize": 12,
  "editor.highlightActiveIndentGuide": false,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {"comments": true, "other": true, "strings": true},
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "all",
  "editor.rulers": [79],
  "editor.scrollBeyondLastLine": true,
  "editor.smoothScrolling": true,
  "editor.wordBasedSuggestions": true,
  "files.autoSave": "off",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "gitlens.advanced.messages": {"suppressShowKeyBindingsNotice": true},
  "python.formatting.blackArgs": [
    "--quiet",
    "--line-length=79",
    "--skip-string-normalization"
  ],
  "python.formatting.provider": "black",
  "python.linting.pylintEnabled": false,
  "python.venvPath": "~/.virtualenvs",
  "search.location": "panel",
  "search.smartCase": true,
  "search.useRipgrep": false,
  "telemetry.enableTelemetry": false,
  "terminal.explorerKind": "external",
  "terminal.external.osxExec": "iTerm.app",
  "window.clickThroughInactive": true,
  "window.nativeTabs": false,
  "window.restoreFullscreen": true,
  "window.restoreWindows": "all",
  "workbench.editor.swipeToNavigate": true,
  "workbench.editor.tabCloseButton": "left",
  "workbench.fontAliasing": "antialiased",
  "workbench.startupEditor": "none",
  "workbench.statusBar.feedback.visible": false,
  "workbench.tree.horizontalScrolling": false,
  "window.zoomLevel": 0,
  "editor.wordWrap": "on",
  "diffEditor.renderSideBySide": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.tags": true,
    "**/tags": true,
    "**/__pycache__": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/__pycache__": true
  },
  "gitlens.gitExplorer.files.layout": "tree",
  "editor.scrollBeyondLastColumn": 0,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.settings.mode": "advanced",
  "gitlens.menus": {
    "editor": {
      "blame": true,
      "clipboard": true,
      "compare": true,
      "details": true,
      "history": true,
      "remote": true
    },
    "editorGroup": {
      "blame": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "editorTab": {
      "compare": true,
      "history": true,
      "remote": true
    },
    "explorer": {
      "compare": true,
      "history": true,
      "remote": true
    }
  },
  "gitlens.gitExplorer.location": "scm",
  "gitlens.explorers.avatars": false,
  "gitlens.historyExplorer.location": "scm",
  "gitlens.resultsExplorer.location": "scm",
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.showTabs": true,
  "workbench.list.multiSelectModifier": "alt",
  "workbench.editor.openSideBySideDirection": "down",
  "searchRipgrep.enable": false,
  "editor.overviewRulerBorder": false,
  "clock.dateFormat": "HH:MM",
  "gitlens.historyExplorer.enabled": false,
  "explorer.confirmDelete": false,
  "python.jediEnabled": true,
  "gitlens.resultsExplorer.files.layout": "tree",
  "workbench.colorTheme": "Plastic",
}
